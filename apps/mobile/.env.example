# Supabase environment variables for the mobile app
SUPABASE_URL="https://your-project-ref.supabase.co"
SUPABASE_ANON_KEY="your-anon-key"

# Firebase Cloud Messaging (FCM) VAPID key for web push notifications
# This is only needed if you support push notifications on the web.
FCM_VAPID_KEY="your-fcm-vapid-key"

# Google Maps API Key
# Replace with your actual Google Maps API key for Android and iOS
GOOGLE_MAPS_API_KEY_ANDROID="your-android-maps-key"
GOOGLE_MAPS_API_KEY_IOS="your-ios-maps-key"
